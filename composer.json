{
    "name": "resultsystems/validation",
    "description": "Inspired 'KennedyTedesco Validation' - The power of 'Respect Validation' on Laravel.",
    "keywords": ["validation", "laravel", "respect", "rules"],
    "license": "MIT",
    "authors": [{
        "name": "Leandro Henrique Reis",
        "email": "emtudo@gmail.com"
    }],
    "require": {
        "php": ">=8.0.0",
        "respect/validation": "2.4.*",
        "illuminate/support": "*"
    },
    "autoload": {
        "psr-4": {
            "ResultSystems\\Validation\\": "src/Validation"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "ResultSystems\\Validation\\ValidationServiceProvider"
            ]
        }
    }
}
